<view class="big" wx:key wx:for="{{testitems}}" wx:for-item="i">
      <radio-group bindchange="checkboxChange" data-id="{{i.id}}" >
          <view class='test-title' >
                {{index+1}}. {{i.title}}
            </view>
            <label class="radio options" wx:key   wx:for="{{i.keywords}}" wx:for-item="j">
                <radio value="{{score[index]}}"  class=""/>  {{arr[index]}}.  {{j}}
            </label>
      </radio-group>
</view>
 <view class="btn">
       <button  bindtap="clickbtn"  class="button" style=""> 下一题</button>
 </view>

