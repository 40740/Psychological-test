<view class="big" wx:key wx:for="{{testitems}}" wx:for-item="i">
      <checkbox-group bindchange="checkboxChange" data-id="{{i.id}}" >
          <view class='test-title' >
                {{index+7}}. {{i.title}}
            </view>
            <label class="checkbox options" wx:key   wx:for="{{i.keywords}}" wx:for-item="j">
                <checkbox value="{{arr[index]}}"  class=""/>  {{arr[index]}}.  {{j}}
            </label>
      </checkbox-group>
</view>
<view class="btn">
       <button type="primary" bindtap="clickbtn"  class="" style="">提交 </button>
</view>

